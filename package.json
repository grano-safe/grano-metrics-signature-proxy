{
  "name": "grano-metrics-signature-proxy",
  "version": "0.0.1",
  "description": "A proxy server to assist in signing requests for Grano Metrics integrations.",
  "author": "AllanOliveiraM <allan@granosafe.dev>",
  "license": "MIT",
  "scripts": {
    "prepare": "sh scripts/setup/prepare.sh",
    "reinstall": "sh scripts/setup/reinstall.sh",
    "clear": "sh scripts/setup/clear.sh",
    "dev": "nodemon -q --watch src --exec tsx src/main.ts --tsconfig ./tsconfig.json",
    "build": "sh scripts/build/build-dist.sh",
    "start": "node dist/index.js",
    "test": "jest",
    "commit": "cz",
    "type:check": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --log-level warn --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint --fix --max-warnings 0 .",
    "eslint:check": "eslint --max-warnings 0 ."
  },
  "dependencies": {
    "@sentry/node": "^8.50.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "chalk": "^5.4.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ms": "^2.1.3",
    "rimraf": "^6.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vercel/ncc": "^0.38.3",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "concurrently": "^8.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "yarn": ">=4.5.1",
    "node": "^20.0.0",
    "npm": "echo \"Please, use yarn\" && exit 1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "micromatch": "^4.0.8"
  },
  "packageManager": "yarn@4.5.1"
}
